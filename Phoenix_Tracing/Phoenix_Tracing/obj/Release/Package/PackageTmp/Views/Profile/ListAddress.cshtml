@model IEnumerable<Phoenix_Tracing.Models.Addresses>

@if (Model != null)
{
    foreach (var item in Model)
    {
        <div class="deepDetail"> Adres İsmi </div>
        <p class="deepDetail2"> @item.name </p>

        <div class="deepDetail"> Adres Telefon Numarası </div>
        <p class="deepDetail2"> @item.phone </p>

        <div class="deepDetail"> Adres </div>
        <p class="deepDetail2">
            @item.address
        </p>
    }
}