@model IEnumerable<Phoenix_Tracing.Models.TotalWork.Periodic_total>

@if (Model != null)
{
    <div class="box">
        <div class="right-side">
            <div class="box-topic">Periyodik İş Talepleri</div>
            <div class="number">
                @foreach (var item in Model)
                {
                    @*@item.periodicTotal*@
                    <p id="periodik_total">@item.periodicTotal</p>
                }
            </div>
        </div>
        <i class='bx bx-plus-circle cart'></i>
    </div>
}