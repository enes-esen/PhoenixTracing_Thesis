@model IEnumerable<Phoenix_Tracing.Models.Users>

@if (Model != null)
{
    foreach (var item in Model)
    {
        <div class="deepDetail"> İsim </div>
        <p class="deepDetail2"> @item.name </p>

        <div class="deepDetail"> Soyisim </div>
        <p class="deepDetail2"> @item.surname </p>

        <div class="deepDetail"> E-mail </div>
        <p class="deepDetail2"> @item.mail </p>

        <div class="deepDetail"> Telefon Numarası </div>
        <p class="deepDetail2"> @item.phone </p>
    }
}