@model IEnumerable<Phoenix_Tracing.Models.Help_request>

@if (Model != null)
{
    <div class="boxes2 box">
        <div class="title">Destek Taleplerim</div>
        <div class="details" id="destekTalep">
                <ul class="details2 text-over">
                    <li class="topic">Destek Talep Konusu</li>
                        @foreach (var item in Model)
                        {
                            <li><a href="#">@item.name</a></li>

                        }
                </ul>
                <ul class="details2 text-over">
                    <li class="topic">Detay</li>
                    @foreach (var item in Model)
                    {
                        <li>
                            <a href="#">@item.detail</a>
                        </li>
                    }
                </ul>
                <ul class="details2 text-over">
                    <li class="topic">Durum</li>
                    @foreach (var item in Model)
                    {
                        <li><a href="#">@item.statusName</a></li>

                    }
                </ul>
        </div>
        <div class="button">
            <a href="~/HelpRequestList/Index">Tamamını Görüntüle</a>
        </div>
        <div class="button">
            <a href="~/HelpRequest/Index">Destek Talebinde Bulun</a>
        </div>
    </div>
}