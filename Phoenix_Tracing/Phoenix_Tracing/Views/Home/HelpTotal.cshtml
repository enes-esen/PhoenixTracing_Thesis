@model IEnumerable<Phoenix_Tracing.Models.TotalWork.Help_total>

@if (Model != null)
{
    <div class="box">
        <div class="right-side">
            <div class="box-topic">Toplam Destek Talebi</div>
            <div class="number">
                @foreach (var item in Model)
                {
                    @*@item.helpTotal*@
                    <p id="help_total">@item.helpTotal</p>
                }
            </div>
        </div>
        <i class='bx bxs-help-circle cart'></i>
    </div>
}